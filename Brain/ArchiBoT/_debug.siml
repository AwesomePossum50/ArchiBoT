<?xml version="1.0" encoding="utf-8"?>
<Siml>
	<Concept Name="_debug">
		<Model>
			<Pattern>DEBUGID GET</Pattern>
			<Response>Your user's "ID" is: "<User Get="ID"/>". The "sourceID" is: "<User Get="SourceID"/>".</Response>
		</Model>
	</Concept>

	<Concept Name="kale test">
		<Model>
			<Pattern>WORDAT *</Pattern>
			<Response>Word count: <Text Get="wordcount"><Match /></Text>. The first word is: <Text WordAt="1"><Match /></Text></Response>
		</Model>

		<Model>
			<Pattern>DEBUG ISPRIVATECHAT</Pattern>
			<Response>
				<Think><User Set="isBotPrivateChat"><Js>"<User Get="ID"/>"==="<User Get="SourceID"/>";</Js></User></Think>
				<Switch User="isBotPrivateChat">
					<Case Value="True">You're in private chat... It worked :speak:</Case>
					<Case Value="False">You're in a group chat... Did it work? :speak:</Case>
					<Default>Something went wrong :cry: <User Get="isBotPrivateChat" /></Default>
				</Switch>
			</Response>
		</Model>

		<Model><Pattern>MAKEARRAY *</Pattern><Response><Js>arrayofnumbers("<Match />")</Js></Response></Model>

		<Model>
			<Pattern>DEBUGGOTO *</Pattern>
			<Response>
				<Think><Var Set="count">0</Var></Think>
				<Bind Key="{0}"><Match /></Bind>
				<While Var="count" Not="{0}"><Think><Var Set="count"><Math Get="increment"><Var Get="count" /></Math></Var></Think><Goto>DEBUGROLL</Goto> </While>
			</Response>
		</Model>

		<Model>
			<Pattern>DEBUGGOTO2 *</Pattern>
			<Response>
				<Think><Var Set="count">0</Var></Think>
				<Bind Key="{0}"><Match /></Bind>
				<While Var="count" Not="{0}"><Think><Var Set="count"><Math Get="increment"><Var Get="count" /></Math></Var></Think><Goto>DEBUGROLL</Goto><Goto>DEBUGROLL</Goto><Goto>DEBUGROLL</Goto> </While>
			</Response>
		</Model>

		<Model>
			<Pattern>DEBUGLATE *</Pattern>
			<Response>I will try to send the message "Success!" in <Match /> seconds.
				<Late Second="{Bind Match}">Success!</Late>
			</Response>
		</Model>
<!-- edit to test target-id in the future -->
		<Model>
			<Pattern>DEBUGLATESELF *</Pattern>
			<Response><Think><Bind Key="{source-id}"><User Get="SourceID" /></Bind></Think>I will try to send the message "<Text>{source-id}</Text> Success!" in <Match /> seconds.
				<Late Second="{Bind Match}"><Text>{source-id}</Text> Success!</Late>
			</Response>
		</Model>

		<Model>
			<Pattern>DEBUGLATE2HU *</Pattern>
			<Response>I will try to send you a message in the Touhou Chat in <Match /> seconds.
				<Late Second="{Bind Match}">Did you get this message in Touhou chat? :sgsmile:</Late>
			</Response>
		</Model>

		<Model>
			<Pattern>DEBUGSILENTLATE *</Pattern>
			<Response><Think><Bind Key="{0}"><Match /></Bind></Think><Late Second="{0}">Success!</Late></Response>
		</Model>

<Model>
<Pattern>COUNT TO *</Pattern>
<Response>
<Bind Key="{0}"><Match /></Bind>
<Var Set="count">0</Var> <While Var="count" Not="{0}"> <Var Set="count"><Math Get="increment"><Var Get="count" /></Math></Var> </While>
</Response>
</Model>

		<Model>
			<Pattern>DEBUGROLL</Pattern>
			<Response>
				<Random>
					<Item>1</Item>
					<Item>2</Item>
					<Item>3</Item>
					<Item>4</Item>
					<Item>5</Item>
					<Item>6</Item>
				</Random>
			</Response>
		</Model>

		<Model>
			<Pattern>BADRANDOM *</Pattern>
			<Response>
				<Think><Var Set="count">0</Var></Think>
				<Bind Key="{0}"><Match /></Bind>
				<While Var="count" Not="{0}"><Think><Var Set="count"><Math Get="increment"><Var Get="count" /></Math></Var></Think>:dice<Random><Item>1</Item><Item>2</Item><Item>3</Item><Item>4</Item><Item>5</Item><Item>6</Item></Random>:</While>
			</Response>
		</Model>

		<Model>
			<Pattern>GOODRANDOM *</Pattern>
			<Response>
				<Think><Var Set="count">0</Var></Think>
				<Bind Key="{0}"><Match /></Bind>
				<While Var="count" Not="{0}"><Think><Var Set="count"><Math Get="increment"><Var Get="count" /></Math></Var></Think>:dice<Random>(1|2|3|4|5|6):</Random></While>
			</Response>
		</Model>

		<Model>
			<Pattern>DEBUG35 *</Pattern>
			<Response>Var set to <Var Set="debug35"><Match /></Var></Response>
		</Model>
		
		<Model>
			<Pattern>DEBUG36 *</Pattern>
			<Response>Var is <Var Get="debug35" /></Response>
		</Model>
	</Concept>

	<Concept Name="personaldebug" User="steam-76561198025147895">
		<Model>
			<Pattern>DEBUGTEST</Pattern>
			<Response>Test successful :speak:</Response>
		</Model>
	</Concept>

	<Concept Name="publicpersonaldebug">
		<Model>
			<Pattern>DEBUGTEST</Pattern>
			<Response>Nope :cry:</Response>
		</Model>
	</Concept>
</Siml>
