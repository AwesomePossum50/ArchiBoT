    <Model>
      <Pattern>_</Pattern>
      <Response>
        <Think>
          <User Set="star">
            <Match />
          </User>
        </Think>
        <Think>
          <User Set="concept">QUESTIONS</User>
        </Think>
        <If User="star" Value="A">	Wrong! Try again!<Goto>QUESTION I</Goto></If>
